<?xml version="1.0" encoding="utf-8"?>

<Defs>

	<ThingDef ParentName="ResourceBase">
		<defName>chrisb_moas_sticks</defName>
		<label>meatless stick</label>
		<description>A pointy stick made from wood. It does not contain any meat.

It can be used to skewer and cook food at a campfire, without burning your podgy little fingies.</description>
		<graphicData>
      <texPath>Items/chrisb_moas_stick</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
		<tradeability>None</tradeability>
		<statBases>
			<MarketValue>0.12</MarketValue>
			<Mass>0.0008</Mass>
		</statBases>
		<thingCategories>
			<li>ResourcesRaw</li>
		</thingCategories>
	</ThingDef>

	<ThingDef ParentName="MealCookedIngredientless">
    <defName>MeatOnAStick_veg</defName>
    <label>veg on a stick</label>
    <description>Vegetables skewered onto a meatless stick and roasted over a fire.</description>
    <graphicData>
      <texPath>Items/chrisb_moas_voas</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <statBases>
      <MarketValue>10</MarketValue>
      <WorkToMake>190</WorkToMake>
      <Nutrition>0.9</Nutrition>
    </statBases>
    <ingestible>
      <preferability>MealSimple</preferability>
      <ingestEffect>EatVegetarian</ingestEffect>
      <ingestSound>Meal_Eat</ingestSound>
    </ingestible>
    <comps>
      <li Class="CompProperties_Ingredients">
        <noIngredientsFoodKind>NonMeat</noIngredientsFoodKind>
      </li>
      <li Class="MeatOnAStick.CompProperties_NameGen"></li>
    </comps>
  </ThingDef>

	<ThingDef ParentName="MealCookedIngredientless">
    <defName>MeatOnAStick_meatless</defName>
    <label>cooked meatless, vegless stick</label>
    <description>A cooked meatless, vegless stick roasted over a fire.
		
Some moments in life make one wonder, "in just what direction is my life going?"</description>
    <graphicData>
      <texPath>Items/chrisb_moas_meatless</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <statBases>
      <MarketValue>1</MarketValue>
      <WorkToMake>190</WorkToMake>
      <Nutrition>0.01</Nutrition>
    </statBases>
    <ingestible>
      <preferability>RawBad</preferability>
			<tasteThought>AteMeatlessStick</tasteThought>
      <ingestEffect>EatVegetarian</ingestEffect>
      <ingestSound>RawVegetable_Eat</ingestSound>
    </ingestible>
    <comps>
      <li Class="CompProperties_Ingredients">
        <noIngredientsFoodKind>NonMeat</noIngredientsFoodKind>
      </li>
    </comps>
  </ThingDef>

